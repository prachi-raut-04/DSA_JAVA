Code 

class MyQueue {
    Stack<Integer> st = new Stack<>();
    Stack<Integer> hSt = new Stack<>();

    public MyQueue() {
        
    }
    
    public void push(int x) {
        if(st.isEmpty()) {
            st.push(x);
            return;
        }

        while(st.size() > 0) {
            hSt.push(st.pop());
        }

        st.push(x);

        while(hSt.size() > 0) {
            st.push(hSt.pop());
        }
    }
    
