Code 

class Solution {
    public void inorder(List<Integer> res, TreeNode root) {
        if(root == null) {
            return;
        }

        inorder(res, root.left);
        res.add(root.val);
        inorder (res, root.right);
    }

    public List<Integer> inorderTraversal(TreeNode root) {
        List<Integer> res = new ArrayList<>();
        inorder(res, root);
        return res;
    }
}
