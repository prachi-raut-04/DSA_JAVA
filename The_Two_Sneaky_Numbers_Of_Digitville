Code 

class Solution {
    public int[] getSneakyNumbers(int[] nums) {
        boolean isPresent[] = new boolean[nums.length];
        int res[] = new int[2];
        int j = 0;

        for(int i=0; i<nums.length; i++) {
            int curr = nums[i];
            if(isPresent[curr]) {
                res[j++] = nums[i];
            }
            isPresent[curr] = true;
        }

        return res;
    }
}

